{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "indent": [true, "tabs", 2],
        "interface-name": false,
        "ordered-imports": false,
        "no-namespace": false,
        "object-literal-sort-keys": {
            "severity": "warning"
        },
        "no-var-requires": {
            "severity": "warning"
        },
        "arrow-parens": false
    },
    "rulesDirectory": []
}
